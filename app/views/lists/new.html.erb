<div class="container">
  <div style="display: flex; justify-content: space-between">
    <h1>Add new list</h1>
    <%= link_to 'Back', lists_path, class:"btn btn-gradient" %>
  </div>
  <%= simple_form_for(@list) do |f| %>
  <%= f.input :name, placeholder: 'list name' %>
  <%= f.submit %>
  <% end %>
</div>
